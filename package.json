    "connect-mongo": "^4.6.0",
    "cookie-parser": "~1.4.4",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "express-validator": "^6.13.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.7",
    "nodemailer": "^6.7.2",
    "tracer": "^1.1.5",
    "websocket": "^1.0.34"
  },
  "type": "module"
}
